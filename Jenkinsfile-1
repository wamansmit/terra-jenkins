
pipeline{
     agent any

     tools{
         jdk 'jdk17'
         maven 'maven'
     }
   

     stages {
         stage('Clean Workspace'){
             steps{
                 cleanWs()
             }
         }
         stage('Checkout from Git'){
             steps{
                 git branch: 'main', url: 'https://github.com/wamansmit/terra-jenkins.git'
             }
         }
  }
}
